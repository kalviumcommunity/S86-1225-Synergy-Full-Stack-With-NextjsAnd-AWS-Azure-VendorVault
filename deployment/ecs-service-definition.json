{
  "cluster": "vendorvault-cluster",
  "service": "vendorvault-service",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "taskDefinition": "vendorvault-task",
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-xxxxxxxxx",
        "subnet-yyyyyyyyy"
      ],
      "securityGroups": [
        "sg-xxxxxxxxx"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:ap-south-1:<ACCOUNT_ID>:targetgroup/vendorvault-tg/xxxxxxxxxxxx",
      "containerName": "vendorvault-app",
      "containerPort": 3000
    }
  ],
  "healthCheckGracePeriodSeconds": 60,
  "enableExecuteCommand": true
}
